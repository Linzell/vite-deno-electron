{
  "name": "vite-deno-electron",
  "private": true,
  "version": "0.0.1",
  "main": "dist-electron/main/index.js",
  "description": "Template for building a Deno/Node app with Vite and Electron",
  "author": "charlie cohen <linzellart@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "release": "vue-tsc --noEmit && vite build -m electron && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2",
    "multiformats": "^12.0.1"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@types/node": "^20.2.5",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.14",
    "electron": "^25.1.0",
    "electron-builder": "^24.4.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.14.1",
    "typescript": "^5.1.3",
    "vite": "^3.2.7",
    "vite-electron-plugin": "^0.8.2",
    "vue-tsc": "^1.6.5"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_HOSTNAME": "127.0.0.1",
      "VITE_DEV_SERVER_PORT": 3344,
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ]
}